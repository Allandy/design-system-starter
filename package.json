{
	"name": "uikit-starterpack",
	"version": "1.0.1",
	"description": "The GOV.AU UI-Kit Starter Pack",
	"main": "index.js",
	"scripts": {
		"build:autoprefix": "postcss docs/css/main.css -u autoprefixer -r -m",
		"build:css": "node-sass src/sass/main.scss docs/css/main.css --output-style compressed",
		"build": "npm run build:css && npm run build:autoprefix",
		"watch:sass": "onchange './src/sass/**/*.scss' -- npm run build",
		"serve": "browser-sync start --server ./docs -w --files \"./docs/index.html\" \"./docs/css/main.css\" \"./docs/js/script.min.js\"",
		"watch": "npm run build && npm run serve | npm run watch:sass"
	},
	"author": {
		"name": "Commonwealth of Australia, DTA",
		"email": "guides@digital.gov.au",
		"url": "https://www.dta.gov.au/"
	},
	"contributors": [
		{
			"name": "Dominik Wilkowski",
			"email": "hi@dominik-wilkowski.com",
			"url": "https://dominik-wilkowski.com/"
		},
		{
			"name": "Alex Page",
			"email": "alex@alexpage.com.au",
			"url": "http://alexpage.com.au"
		},
		{
			"name": "Patrick De Young",
			"email": "hello@patrickdeyoung.me",
			"url": "https://patrickdeyoung.me"
		},
		{
			"name": "Sukhraj Ghuman",
			"email": "sukhrajghuman@live.com",
			"url": "https://github.com/sukhrajghuman"
		}
	],
	"license": "MIT",
	"dependencies": {
		"@gov.au/accordion": "latest",
		"@gov.au/animate": "latest",
		"@gov.au/breadcrumbs": "latest",
		"@gov.au/buttons": "latest",
		"@gov.au/footer": "latest",
		"@gov.au/grid-12": "latest",
		"@gov.au/header": "latest",
		"@gov.au/link-list": "latest",
		"@gov.au/responsive-media": "latest",
		"@gov.au/side-nav": "latest",
		"@gov.au/skip-link": "latest",
		"@gov.au/text-inputs": "latest"
	},
	"devDependencies": {
		"autoprefixer": "^9.0.1",
		"browser-sync": "^2.24.5",
		"node-sass": "^4.9.2",
		"onchange": "^4.1.0",
		"postcss-cli": "^6.0.0"
	},
	"pancake": {
		"auto-save": true,
		"plugins": true,
		"ignore": [],
		"json": {
			"enable": false,
			"location": "pancake/",
			"name": "pancake",
			"content": {
				"name": true,
				"version": true,
				"dependencies": true,
				"path": true,
				"settings": true
			}
		},
		"css": {
			"minified": true,
			"modules": false,
			"browsers": [
				"last 2 versions",
				"ie 8",
				"ie 9",
				"ie 10"
			],
			"location": false,
			"name": false
		},
		"sass": {
			"modules": false,
			"location": "src/sass/",
			"name": "_uikit.scss"
		},
		"react": {
			"location": false
		},
		"js": {
			"minified": true,
			"modules": false,
			"location": "docs/js/",
			"name": "script.js"
		}
	},
	"browserslist": [
		"last 2 versions",
		"ie 8",
		"ie 9",
		"ie 10"
	]
}
